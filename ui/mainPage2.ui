    margin: 0px 0px 0px 0px; /* 边距 */  
}  
  
QScrollBar::handle:vertical {  
    background: #a8a8a8; /* 滑块背景色 */  
    min-height: 20px; /* 最小高度 */  
}  
  
QScrollBar::add-line:vertical {  
    border: none; /* 无边框 */  
    background: none; /* 无背景 */  
    height: 0px; /* 高度 */  
    subcontrol-position: bottom; /* 位置 */  
    subcontrol-origin: margin; /* 起始位置 */  
}  
  
QScrollBar::sub-line:vertical {  
    border: none; /* 无边框 */  
    background: none; /* 无背景 */  
